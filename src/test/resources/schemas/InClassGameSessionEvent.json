{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "id",
    "gameId",
    "gameTitle",
    "players",
    "joinedPlayers",
    "questions",
    "questionCodes",
    "startedOn",
    "startedBy",
    "updatedAt",
    "status",
    "occurredOn"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "default": "",
      "examples": [
        "9b076d5d-db35-44b9-91e5-3db8205c9d43"
      ],
      "pattern": "^(.*)$"
    },
    "gameId": {
      "$id": "#/properties/gameId",
      "type": "string",
      "title": "The Gameid Schema",
      "default": "",
      "examples": [
        "01af02c3-cacd-43fd-bc38-64a7326ffe01"
      ],
      "pattern": "^(.*)$"
    },
    "gameTitle": {
      "$id": "#/properties/gameTitle",
      "type": "string",
      "title": "The Gametitle Schema",
      "default": "",
      "examples": [
        "Mlo title"
      ],
      "pattern": "^(.*)$"
    },
    "players": {
      "$id": "#/properties/players",
      "type": "array",
      "title": "The Players Schema",
      "items": {
        "$id": "#/properties/players/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "f8e901e9-ef22-4ce2-80e1-c0974cd541dc",
          "ddf6d515-cd19-40e7-9d0c-902233e53c07"
        ],
        "pattern": "^(.*)$"
      }
    },
    "joinedPlayers": {
      "$id": "#/properties/joinedPlayers",
      "type": "array",
      "title": "The Joinedplayers Schema"
    },
    "questions": {
      "$id": "#/properties/questions",
      "type": "array",
      "title": "The Questions Schema",
      "items": {
        "$id": "#/properties/questions/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "code",
          "time"
        ],
        "properties": {
          "code": {
            "$id": "#/properties/questions/items/properties/code",
            "type": "string",
            "title": "The Code Schema",
            "default": "",
            "examples": [
              "Q1"
            ],
            "pattern": "^(.*)$"
          },
          "time": {
            "$id": "#/properties/questions/items/properties/time",
            "type": "integer",
            "title": "The Time Schema",
            "default": 0,
            "examples": [
              60
            ]
          }
        }
      }
    },
    "questionCodes": {
      "$id": "#/properties/questionCodes",
      "type": "array",
      "title": "The Questioncodes Schema",
      "items": {
        "$id": "#/properties/questionCodes/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "Q1",
          "Q2",
          "Q3",
          "Q4",
          "Q5"
        ],
        "pattern": "^(.*)$"
      }
    },
    "startedOn": {
      "$id": "#/properties/startedOn",
      "type": "string",
      "title": "The Startedon Schema",
      "default": "",
      "examples": [
        "2020-02-11T13:33:15.926"
      ],
      "pattern": "^(.*)$"
    },
    "startedBy": {
      "$id": "#/properties/startedBy",
      "type": "string",
      "title": "The Startedby Schema",
      "default": "",
      "examples": [
        "87e9d9f3-bfc8-4bf1-9e54-5d7b6488f11b"
      ],
      "pattern": "^(.*)$"
    },
    "updatedAt": {
      "$id": "#/properties/updatedAt",
      "type": "string",
      "title": "The Updatedat Schema",
      "default": "",
      "examples": [
        "2020-02-11T13:33:15.926"
      ],
      "pattern": "^(.*)$"
    },
    "status": {
      "$id": "#/properties/status",
      "type": "string",
      "title": "The Status Schema",
      "default": "",
      "examples": [
        "IN_PROGRESS"
      ],
      "pattern": "^(.*)$"
    },
    "occurredOn": {
      "$id": "#/properties/occurredOn",
      "type": "string",
      "title": "The Occurredon Schema",
      "default": "",
      "examples": [
        "2020-02-11T13:33:15.926"
      ],
      "pattern": "^(.*)$"
    }
  }
}