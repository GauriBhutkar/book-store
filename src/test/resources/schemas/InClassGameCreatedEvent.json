{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "id",
    "schoolId",
    "classId",
    "mloId",
    "title",
    "teacherId",
    "subjectId",
    "schoolGradeId",
    "genSubject",
    "k12Grade",
    "groupId",
    "learningPathId",
    "trimesterId",
    "questions",
    "questionCodes",
    "createdAt",
    "occurredOn"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "default": "",
      "examples": [
        "00fbd2b1-66ba-4182-a2d1-85dc0836fd47"
      ],
      "pattern": "^(.*)$"
    },
    "schoolId": {
      "$id": "#/properties/schoolId",
      "type": "string",
      "title": "The Schoolid Schema",
      "default": "",
      "examples": [
        "a5b074b1-5dc5-43e6-9379-d23b1beaf1e5"
      ],
      "pattern": "^(.*)$"
    },
    "classId": {
      "$id": "#/properties/classId",
      "type": "string",
      "title": "The Classid Schema",
      "default": "",
      "examples": [
        "4cf62491-cdd7-4cdc-ac10-ec0c47e8c8dc"
      ],
      "pattern": "^(.*)$"
    },
    "mloId": {
      "$id": "#/properties/mloId",
      "type": "string",
      "title": "The Mloid Schema",
      "default": "",
      "examples": [
        "2bbfacd5-0177-453b-9c88-1bdc32c01684"
      ],
      "pattern": "^(.*)$"
    },
    "title": {
      "$id": "#/properties/title",
      "type": "string",
      "title": "The Title Schema",
      "default": "",
      "examples": [
        "Mlo title"
      ],
      "pattern": "^(.*)$"
    },
    "teacherId": {
      "$id": "#/properties/teacherId",
      "type": "string",
      "title": "The Teacherid Schema",
      "default": "",
      "examples": [
        "4ce03c5e-5050-4509-b247-4fe71142eda6"
      ],
      "pattern": "^(.*)$"
    },
    "subjectId": {
      "$id": "#/properties/subjectId",
      "type": "string",
      "title": "The Subjectid Schema",
      "default": "",
      "examples": [
        "324b109b-6a45-4488-bfa4-f449b2019027"
      ],
      "pattern": "^(.*)$"
    },
    "schoolGradeId": {
      "$id": "#/properties/schoolGradeId",
      "type": "string",
      "title": "The Schoolgradeid Schema",
      "default": "",
      "examples": [
        "d20ac267-0e59-4aa5-875e-21670378a5c8"
      ],
      "pattern": "^(.*)$"
    },
    "genSubject": {
      "$id": "#/properties/genSubject",
      "type": "string",
      "title": "The Gensubject Schema",
      "default": "",
      "examples": [
        "Math"
      ],
      "pattern": "^(.*)$"
    },
    "k12Grade": {
      "$id": "#/properties/k12Grade",
      "type": "integer",
      "title": "The K12grade Schema",
      "default": 0,
      "examples": [
        6
      ]
    },
    "groupId": {
      "$id": "#/properties/groupId",
      "type": "string",
      "title": "The Groupid Schema",
      "default": "",
      "examples": [
        "12188cfc-05f2-4b88-b5ee-d96332915ef8"
      ],
      "pattern": "^(.*)$"
    },
    "learningPathId": {
      "$id": "#/properties/learningPathId",
      "type": "string",
      "title": "The Learningpathid Schema",
      "default": "",
      "examples": [
        "84d91393-a149-4ba6-b046-bc51e45ae9b3"
      ],
      "pattern": "^(.*)$"
    },
    "trimesterId": {
      "$id": "#/properties/trimesterId",
      "type": "string",
      "title": "The Trimesterid Schema",
      "default": "",
      "examples": [
        "e53ce2f3-dc84-4ce4-b2d6-fdd4feda3250"
      ],
      "pattern": "^(.*)$"
    },
    "questions": {
      "$id": "#/properties/questions",
      "type": "array",
      "title": "The Questions Schema",
      "items": {
        "$id": "#/properties/questions/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "code",
          "time"
        ],
        "properties": {
          "code": {
            "$id": "#/properties/questions/items/properties/code",
            "type": "string",
            "title": "The Code Schema",
            "default": "",
            "examples": [
              "Q1"
            ],
            "pattern": "^(.*)$"
          },
          "time": {
            "$id": "#/properties/questions/items/properties/time",
            "type": "integer",
            "title": "The Time Schema",
            "default": 0,
            "examples": [
              60
            ]
          }
        }
      }
    },
    "questionCodes": {
      "$id": "#/properties/questionCodes",
      "type": "array",
      "title": "The Questioncodes Schema",
      "items": {
        "$id": "#/properties/questionCodes/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "Q1",
          "Q2",
          "Q3",
          "Q4",
          "Q5"
        ],
        "pattern": "^(.*)$"
      }
    },
    "createdAt": {
      "$id": "#/properties/createdAt",
      "type": "string",
      "title": "The Createdat Schema",
      "default": "",
      "examples": [
        "2020-02-11T13:18:48.488"
      ],
      "pattern": "^(.*)$"
    },
    "occurredOn": {
      "$id": "#/properties/occurredOn",
      "type": "string",
      "title": "The Occurredon Schema",
      "default": "",
      "examples": [
        "2020-02-11T13:18:48.488"
      ],
      "pattern": "^(.*)$"
    }
  }
}