{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "id",
    "sessionId",
    "gameId",
    "playerId",
    "mloId",
    "assessmentId",
    "score",
    "scoreBreakdown",
    "createdAt",
    "updatedAt",
    "status",
    "occurredOn"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "default": "",
      "examples": [
        "c4682c02-cb5b-4826-82cc-0eb5c78f025f"
      ],
      "pattern": "^(.*)$"
    },
    "sessionId": {
      "$id": "#/properties/sessionId",
      "type": "string",
      "title": "The Sessionid Schema",
      "default": "",
      "examples": [
        "31db7752-9685-4f91-9306-95894892d7be"
      ],
      "pattern": "^(.*)$"
    },
    "gameId": {
      "$id": "#/properties/gameId",
      "type": "string",
      "title": "The Gameid Schema",
      "default": "",
      "examples": [
        "998d9605-c22f-4f25-801b-663a49021161"
      ],
      "pattern": "^(.*)$"
    },
    "playerId": {
      "$id": "#/properties/playerId",
      "type": "string",
      "title": "The Playerid Schema",
      "default": "",
      "examples": [
        "f15a5f55-a8cc-426e-bc4c-22be90b4da08"
      ],
      "pattern": "^(.*)$"
    },
    "mloId": {
      "$id": "#/properties/mloId",
      "type": "string",
      "title": "The Mloid Schema",
      "default": "",
      "examples": [
        "0db22d7a-4cee-4081-b477-2bdb3cf65ec7"
      ],
      "pattern": "^(.*)$"
    },
    "assessmentId": {
      "$id": "#/properties/assessmentId",
      "type": "string",
      "title": "The Assessmentid Schema",
      "default": "",
      "examples": [
        "a6d5be00-0763-4e25-b1bc-b75b65ee6881"
      ],
      "pattern": "^(.*)$"
    },
    "score": {
      "$id": "#/properties/score",
      "type": "number",
      "title": "The Score Schema",
      "default": 0,
      "examples": [
        100
      ]
    },
    "scoreBreakdown": {
      "$id": "#/properties/scoreBreakdown",
      "type": "array",
      "title": "The Scorebreakdown Schema",
      "items": {
        "$id": "#/properties/scoreBreakdown/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "code",
          "score",
          "timeSpent",
          "createdAt",
          "updatedAt"
        ],
        "properties": {
          "code": {
            "$id": "#/properties/scoreBreakdown/items/properties/code",
            "type": "string",
            "title": "The Code Schema",
            "default": "",
            "examples": [
              "Q1"
            ],
            "pattern": "^(.*)$"
          },
          "score": {
            "$id": "#/properties/scoreBreakdown/items/properties/score",
            "type": "number",
            "title": "The Score Schema",
            "default": 0,
            "examples": [
              100
            ]
          },
          "timeSpent": {
            "$id": "#/properties/scoreBreakdown/items/properties/timeSpent",
            "type": "integer",
            "title": "The Timespent Schema",
            "default": 0,
            "examples": [
              50
            ]
          },
          "createdAt": {
            "$id": "#/properties/scoreBreakdown/items/properties/createdAt",
            "type": "string",
            "title": "The Createdat Schema",
            "default": "",
            "examples": [
              "2020-02-11T13:45:32.586"
            ],
            "pattern": "^(.*)$"
          },
          "updatedAt": {
            "$id": "#/properties/scoreBreakdown/items/properties/updatedAt",
            "type": "string",
            "title": "The Updatedat Schema",
            "default": "",
            "examples": [
              "2020-02-11T13:45:32.727"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    },
    "createdAt": {
      "$id": "#/properties/createdAt",
      "type": "string",
      "title": "The Createdat Schema",
      "default": "",
      "examples": [
        "2020-02-11T13:45:32.586"
      ],
      "pattern": "^(.*)$"
    },
    "updatedAt": {
      "$id": "#/properties/updatedAt",
      "type": "string",
      "title": "The Updatedat Schema",
      "default": "",
      "examples": [
        "2020-02-11T13:45:32.727"
      ],
      "pattern": "^(.*)$"
    },
    "status": {
      "$id": "#/properties/status",
      "type": "string",
      "title": "The Status Schema",
      "default": "",
      "examples": [
        "COMPLETED"
      ],
      "pattern": "^(.*)$"
    },
    "occurredOn": {
      "$id": "#/properties/occurredOn",
      "type": "string",
      "title": "The Occurredon Schema",
      "default": "",
      "examples": [
        "2020-02-11T13:45:32.727"
      ],
      "pattern": "^(.*)$"
    }
  }
}